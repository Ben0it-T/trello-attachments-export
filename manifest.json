{
    "name": "Trello attachments export",
    "version": "0.1",
    "manifest_version" : 3,
    "description": "Chrome extension for Trello that allows to download all attachments on a Board",
    "content_scripts": [
        {
            "matches": [
                "https://trello.com/*"
            ],
            "css": [
                "lib/jquery.growl.css"
            ],
            "js": [
                "lib/jquery-3.6.3.min.js",
                "lib/jquery.growl.js",
                "trello-attachments-export.js"
             ]
        }
    ]
}